{
    "name": "codicastudio/sluggable",
    "description": "A noobs worthless garbage, just learning.",
    "keywords": [
        "worthless garabage by a noob."
    ],
    "homepage": "https://github.com/codicastudio/sluggable",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "codicastudiodev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4.8",
        "cocur/slugify": "^4.0",
        "illuminate/config": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "limedeck/phpunit-detailed-printer": "^5.0",
        "mockery/mockery": "^1.2.3",
        "orchestra/database": "^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "codicastudio\\sluggable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "codicastudio\\sluggable\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\sluggable\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
